{
  "manifest_version": 3,
  "name": "Infinity Block",
  "description": "Block out distracting sites. Get work done.",
  "version": "1.0.0",
  "icons": {
    "16": "app/assets/app_icon_16.png",
    "48": "app/assets/app_icon_48.png",
    "128": "app/assets/app_icon_128.png"
  },
  "action": {
    "default_icon": "app/assets/app_icon_16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [],
  "permissions": [
    "tabs",
    "storage"
  ]
}